<%include head%>
    <!--登陆之后的页面-->

    
    <div class="container-fluid" ng-controller="IndexCtrl">
         <!-- <div class="row-fluid"> -->
         	   <div class="span4"></div>
         	   <div class="span2">
         	   	    <!-- 头像 -->
         	   	    <div class="my-nav-user text-center">
